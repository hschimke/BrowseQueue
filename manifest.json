{
  "name": "Browse Queue",
  "version": "1.0.1.0",
  "description": "Queue based browsing.",
  "background_page": "bg.html",
  "options_page": "options.html",
  "icons": { "48": "disp_ico_48.png", "128": "disp_ico_128.png" },
  "permissions": [
    "bookmarks", "tabs"
  ],
  "browser_action": {
	"default_title": "Queue Control",
	"popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["keyboard_shortcuts.js"]
    }
  ]
}
